{
  "name": "electron-express-angular",
  "version": "1.2.1",
  "main": "main.js",
  "scripts": {
    "start": "npm run build && electron main",
    "win32": "npm run build && electron-packager . myApp --platform=win32 --arch=ia32",
    "win64": "npm run build && electron-packager . myApp --platform=win32 --arch=x64",
    "linux32": "npm run build && electron-packager . myApp --platform=linux --arch=ia32",
    "linux64": "npm run build && electron-packager . myApp --platform=linux --arch=x64",
    "osx": "npm run build && electron-packager . myApp --platform=darwin --arch=x64",
    "ng": "ng",
    "build": "pushd angular && ng build && popd",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "devDependencies": {
    "electron": "^4.0.8",
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  },
  "dependencies": {
    "electron-packager": "^13.1.1",
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "morgan": "^1.9.1"
  },
  "private": true
}
