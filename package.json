{
  "name": "electron-express-angular",
  "description": "Nice and steady boilerplate for Angular and Express.js in Electron",
  "version": "1.3.0",
  "main": "main.js",
  "scripts": {
    "start": "npm run build && electron main",
    "win32": "npm run build && electron-packager . --asar --platform=win32 --arch=ia32 --prune=true --out=electron",
    "win64": "npm run build && electron-packager . --asar --platform=win32 --arch=x64 --prune=true --out=electron",
    "linux32": "npm run build && electron-packager . --asar --platform=linux --arch=ia32 --prune=true --out=electron",
    "linux64": "npm run build && electron-packager . --asar --platform=linux --arch=x64 --prune=true --out=electron",
    "osx": "npm run build && electron-packager . --asar --platform=darwin --arch=x64 --prune=true --out=electron",
    "ng": "ng",
    "build": "pushd angular && ng build && popd",
    "test": "echo \"Error: no test specified\" && exit 1",
    "e2e": "echo \"Error: no e2e specified\" && exit 1",
    "lint": "ng lint"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "electron": "^4.0.8",
    "electron-winstaller": "^3.0.4",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  },
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "core-js": "^2.5.4",
    "electron-packager": "^13.1.1",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "extraFiles": [
    "electron/windows/setupevents.js"
  ],
  "private": true
}
